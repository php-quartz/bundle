{
    "name": "php-quartz/bundle",
    "type": "symfony-bundle",
    "description": "Quartz Remote Scheduler Bundle",
    "keywords": ["job", "time", "task", "time scheduler", "quartz", "chrono"],
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "symfony/framework-bundle": "^2.8|^3",
        "symfony/console": "^2.8|^3",
        "enqueue/enqueue-bundle": "^0.7@dev",
        "php-quartz/bridge": "^0.1@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.5",
        "symfony/browser-kit": "^2.8|^3",
        "enqueue/null": "^0.5@dev"
    },
    "autoload": {
        "psr-4": { "Quartz\\Bundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
