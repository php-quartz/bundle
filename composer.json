{
    "name": "php-quartz/bundle",
    "type": "symfony-bundle",
    "description": "Quartz Remote Scheduler Bundle",
    "keywords": ["job", "time", "task", "time scheduler", "quartz", "chrono"],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "symfony/framework-bundle": "^3|^4",
        "symfony/console": "^3|^4",
        "enqueue/enqueue-bundle": "^0.9",
        "php-quartz/bridge": "^0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5",
        "symfony/browser-kit": "^3|^4",
        "enqueue/null": "^0.9"
    },
    "autoload": {
        "psr-4": { "Quartz\\Bundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
}
